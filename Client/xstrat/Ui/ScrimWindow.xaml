<Window x:Class="xstrat.Ui.ScrimWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:xstrat.Ui"
        mc:Ignorable="d"
        Title="Scrim" Height="400" Width="700" Background="#161616" ShowInTaskbar="False" WindowStartupLocation="CenterOwner">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Label Name="TypeLabel" Foreground="#336cb5" HorizontalAlignment="Center" FontSize="28"></Label>
            
        </StackPanel>
    </Grid>
</Window>
