<UserControl x:Class="xstrat.Ui.UserCheckbox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:xstrat.Ui"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="200">
    <Grid>
        <CheckBox Foreground="White" FontSize="16" VerticalAlignment="Center" VerticalContentAlignment="Center" Name="UserCheckboxItem" Background="#336cb5"></CheckBox>
    </Grid>
</UserControl>
